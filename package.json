{
  "name": "void-web-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffect": false,
  "scripts": {
    "_predev": "yarn graphql-codegen",
    "dev": "next dev",
    "node:dev": "node server.mjs",
    "_prebuild": "rimraf .next && yarn graphql-codegen",
    "build": "next build",
    "prestart": "yarn graphql-codegen",
    "start": "next start",
    "node:start": "cross-env NODE_ENV=production node server.mjs",
    "prelint": "yarn format",
    "lint": "concurrently -n tsc,es -c magenta,cyan \"yarn type-check\" \"yarn lint:es\"",
    "lint:fix": "next lint --fix",
    "lint:es": "next lint --quiet",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "type-check": "tsc",
    "graphql-codegen": "graphql-codegen --config .graphqlrc",
    "prepare": "husky install"
  },
  "dependencies": {
