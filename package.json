    "@mrii/react-form-builder": "^1.0.7",
    "@mrii/react-mousetrap-hook": "^1.0.1",
    "@mrii/react-table-builder": "^0.0.5",
    "@mui/icons-material": "^5.11.16",
    "@mui/lab": "^5.0.0-alpha.128",
    "@mui/material": "^5.12.2",
    "@mui/x-data-grid": "^6.3.0",
    "@mui/x-date-pickers": "^6.3.0",
    "@react-google-maps/api": "^2.18.1",
    "@tanstack/react-query": "^4.29.5",
    "add": "^2.0.6",
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "date-fns": "^2.30.0",
    "fast-memoize": "^2.5.2",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "immer": "^10.0.1",
    "lodash.get": "^4.4.2",
    "memoize-one": "^6.0.0",
    "mousetrap": "^1.6.5",
    "next": "13.3.2",
    "next-auth": "^4.22.1",
    "next-seo": "^6.0.0",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
