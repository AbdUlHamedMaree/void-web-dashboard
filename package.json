  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-eslint/eslint-plugin": "^3.18.0",
    "@types/lodash.get": "^4.4.7",
    "@types/mousetrap": "^1.6.11",
    "@types/node": "18.16.3",
    "@types/nprogress": "^0.2.0",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
